{
  "name": "rat-battle-auth",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "_moduleAliases": {
    "src": "functions"
  },
  "scripts": {
    "compile": "pnpm codegen && tsc && tsc && cp package.json functions/package.json",
    "codegen": "graphql-codegen",
    "format": "prettier -w \"./src/**/*.(js|ts|json|gql)\"",
    "dev": "pnpm codegen && concurrently --kill-others --prefix \"[{name}]\" --names \"TSC,APP\" \"tsc -w\" \"nodemon functions/dev.js\"",
    "deploy": "pnpm run compile && pnpm run gcloud --prefix functions/",
    "deploy:dev": "pnpm run compile && pnpm run gcloud:dev --prefix functions/",
    "gcloud": "gcloud functions deploy battle-actions --project=awwrats --entry-point=app --trigger-http --runtime=nodejs14 --allow-unauthenticated --region=us-east4",
    "gcloud:dev": "gcloud functions deploy battle-actions-dev --project=awwrats --entry-point=app --trigger-http --runtime=nodejs14 --allow-unauthenticated --region=us-east4",
    "test-actions": "act --secret-file .secrets.env",
    "test": "jest --silent"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@graphql-codegen/introspection": "^2.1.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.4",
    "@types/supertest": "^2.0.11",
    "concurrently": "^6.5.1",
    "eslint": "^8.5.0",
    "jest": "^27.4.7",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.4",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "@google-cloud/functions-framework": "^2.1.0",
    "@graphql-codegen/cli": "^2.3.0",
    "@graphql-codegen/typescript": "^2.4.1",
    "@graphql-codegen/typescript-graphql-request": "^4.3.2",
    "@graphql-codegen/typescript-operations": "^2.2.1",
    "axios": "^0.24.0",
    "cookie-parser": "^1.4.6",
    "date-fns": "^2.27.0",
    "dotenv": "^10.0.0",
    "ethereumjs-util": "^7.1.3",
    "ethers": "^5.5.2",
    "express": "^4.17.2",
    "graphql": "15.7.2",
    "graphql-request": "^3.7.0",
    "graphql-tag": "^2.12.6",
    "module-alias": "^2.2.2",
    "morgan": "^1.10.0",
    "rlp": "^2.2.7"
  }
}
